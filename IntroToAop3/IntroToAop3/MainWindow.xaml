<Window x:Class="IntroToAop3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Intro to AOP 3" Height="156" Width="417">
    <Grid>
        <TextBox x:Name="txtBreakIt" HorizontalAlignment="Left" Height="23" Margin="188,20,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Enter Stuff to Break the UI:" HorizontalAlignment="Left" Margin="6,17,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnBreakIt" Content="Break It" HorizontalAlignment="Left" Margin="324,21,0,0" VerticalAlignment="Top" Width="75" Click="btnBreakIt_Click" />
        <TextBox x:Name="txtDontBreakIt" HorizontalAlignment="Left" Height="23" Margin="188,57,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Enter Stuff to Not Break the UI:" HorizontalAlignment="Left" Margin="6,54,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnDontBreakIt" Content="Work It" HorizontalAlignment="Left" Margin="324,57,0,0" VerticalAlignment="Top" Width="75" Click="btnDontBreakIt_Click"/>

    </Grid>
</Window>
